  "paths": {
    "/auth/admin/login": {
      "parameters": [],
      "post": {
        "summary": "Login Admin",
        "parameters": [
          {
            "name": "Content-Type",
            "in": "header",
            "required": false,
            "example": "application/json",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "headers": {
              "X-Powered-By": {
                "schema": {
                  "type": "string"
                },
                "example": "Express"
              },
              "Content-Type": {
                "schema": {
                  "type": "string"
                },
                "example": "application/json; charset=utf-8"
              },
              "Content-Length": {
                "schema": {
                  "type": "integer"
                },
                "example": "317"
              },
              "ETag": {
                "schema": {
                  "type": "string"
                },
                "example": "W/\"13d-uZ7BCeTqf61tsZYYjzqUVXTf2zM\""
              },
              "Date": {
                "schema": {
                  "type": "string"
                },
                "example": "Sat, 14 Jun 2025 01:28:30 GMT"
              },
              "Connection": {
                "schema": {
                  "type": "string"
                },
                "example": "keep-alive"
              },
              "Keep-Alive": {
                "schema": {
                  "type": "string"
                },
                "example": "timeout=5"
              }
            },
            "description": "berhasil login",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "token": {
                          "type": "string"
                        },
                        "admin": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "string"
                            },
                            "username": {
                              "type": "string"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
